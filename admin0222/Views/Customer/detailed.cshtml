@model admin0222.Models.Customer
@{
    ViewBag.Title = "detailed";
}

<h2>客戶詳細資料</h2>
<div>


    <h4>
        <dl class="dl-horizontal">

            <dt>
                客戶公司名 : @Html.DisplayFor(model => model.Name)
            </dt>


            <dt>
                公司地址 : @Html.DisplayFor(model => model.Addrress)
            </dt>


            <dt>
                公司聯絡電話 : @Html.DisplayFor(model => model.Tel)
            </dt>


            <dt>
                公司統一編號 :  @Html.DisplayFor(model => model.Code)
            </dt>


        </dl>
    </h4>
   
</div>

<div>
    @Html.ActionLink("返回列表", "Read")
</div>

